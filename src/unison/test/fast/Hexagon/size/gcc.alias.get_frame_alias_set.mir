    S2_storeriabs @get_frame_alias_set.set, %2, <0x2772240> = !{!"unison-property", !"constant-extended"}, <0x2772200> = !{!"unison-memory-partition", i32 0} :: (store 4 into @get_frame_alias_set.set)
  
