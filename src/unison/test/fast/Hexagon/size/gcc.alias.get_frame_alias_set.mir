    %6 = L4_loadri_abs @flag_strict_aliasing, <0x2772240> = !{!"unison-property", !"constant-extended"}, <0x2772200> = !{!"unison-memory-partition", i32 0} :: (load 4 from @flag_strict_aliasing)
    %7 = C2_cmpeqi killed %6, 0
